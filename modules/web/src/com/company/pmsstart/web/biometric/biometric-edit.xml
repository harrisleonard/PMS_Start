<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.pmsstart.web.biometric.BiometricEdit"
        datasource="biometricDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.pmsstart.web.biometric">
    <dsContext>
        <datasource id="biometricDs"
                    class="com.company.pmsstart.entity.Biometric"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="biometricDs">
            <column width="250px">
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
