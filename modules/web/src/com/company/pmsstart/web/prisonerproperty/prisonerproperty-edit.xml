<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.pmsstart.web.prisonerproperty.PrisonerpropertyEdit"
        datasource="prisonerpropertyDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.pmsstart.web.prisonerproperty">
    <dsContext>
        <datasource id="prisonerpropertyDs"
                    class="com.company.pmsstart.entity.Prisonerproperty"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="prisonerpropertyDs">
            <column width="250px">
                <field caption="Nature Of Property"
                       property="natureofproperty"/>
                <field property="remarks"/>
            </column>
            <column>
                <field caption="Serial Number"
                       property="serialnumber"/>
            </column>
            <column>
                <field property="description"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
