<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.pmsstart.web.foreignaddress.ForeignaddressEdit"
        datasource="foreignaddressDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.pmsstart.web.foreignaddress">
    <dsContext>
        <datasource id="foreignaddressDs"
                    class="com.company.pmsstart.entity.Foreignaddress"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="foreignaddressDs">
            <column width="250px">
                <field property="country"/>
            </column>
            <column>
                <field property="town"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
